<div class="container">
    <div class="row">
        <div class="col-sm-4" style=" left:-20px;">
            <!--<h1>Manage Customers</h1>-->
            <!-- Search customers -->
            <h3>Search Brand</h3>
            <div class="input-group">
                <input type="text" ng-model="searchText" class="form-control" placeholder="Enter search text">
                <span class="input-group-addon" id="basic-addon">

                    <i class="glyphicon glyphicon-search" ng-click="searchAnyBrand()"></i>

                </span>

            </div>
        </div>


        

        <div class="col-sm-6">

        </div>
        <div class="col-sm-2">
            <button ng-click="addBrand()" class="btn btn-success btn-sm">
                <i class="glyphicon glyphicon-plus"></i> Add Brand
            </button>
        </div>



    </div>
    <div class="row"> <p></p> </div>
    
    <div class="row">

        <div class="col-sm-3" style="padding-top:3px; border:1px solid #999; height:450px; max-height:500px; overflow-y:scroll;" id="panelheightlist">

            <br />
            <div class="col-sm-12 " ng-repeat="brand in brands">
                <as-brand></as-brand>
            </div>

        </div>
       
        <div class="col-sm-9" style="border:1px solid #999; height:450px; ">

            <div ui-view></div>
            

        </div>
    
    </div>
</div>